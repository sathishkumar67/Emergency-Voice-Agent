    "import sqlite3\n",
    "import pandas as pd\n",
    "\n",
    "# Connect\n",
    "conn = sqlite3.connect('emergency_data.db')\n",
    "\n",
    "# Read SQL into DataFrame\n",
    "df = pd.read_sql_query(\"SELECT * FROM incidents\", conn)\n",
    "\n",
    "# Display\n",
    "print(df)\n",
    "\n",
    "# Close\n",
    "conn.close()"
   ]
